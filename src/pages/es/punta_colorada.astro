---
import Layout from "@/layouts/Layout.astro"
---

<Layout pageTitle="Casa_Comporta">
  <figure class="layout_temporal">
    <img src="../../assets/work/punta_colorada/Frame14225.jpg" alt="">
  </figure>
</Layout>

<style>
  @media (width <= 992px) {
    .layout_temporal {
      display: none !important;
      img {
        display: none !important;
      }
    }
  }
  .layout_temporal {
    display: block;
    min-height: 500rem;
    width: 100%;
    height: max-content;
    img {
      width: 100%;
      height: 100%;
    }
  }
</style>